{
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config",
    "github>aquaproj/aqua-renovate-config:installer-script(install-aqua\\.sh)"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +name: (?<depName>[^'\" @/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*@(?<currentValue>[^'\" \\n]+)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
